{
  "name": "fscrape",
  "version": "1.0.0",
  "description": "A flexible forum scraper supporting Reddit with extensible platform architecture",
  "main": "dist/index.js",
  "bin": {
    "fscrape": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neonwatty/fscrape.git"
  },
  "keywords": ["scraper", "reddit", "forum", "data-extraction", "cli", "typescript"],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/neonwatty/fscrape/issues"
  },
  "homepage": "https://github.com/neonwatty/fscrape#readme",
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.2",
    "p-limit": "^7.1.1",
    "p-retry": "^7.0.0",
    "playwright": "^1.55.0",
    "winston": "^3.17.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.3.1",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
